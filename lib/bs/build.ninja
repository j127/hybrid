g_pkg_flg = -bs-package-name your-project-name -bs-ns YourProjectName
src_root_dir = /home/pv/code/REASON_EXPERIMENTS/hybrid
bsc = /home/pv/code/REASON_EXPERIMENTS/hybrid/node_modules/bs-platform/linux/bsc.exe
bsdep = /home/pv/code/REASON_EXPERIMENTS/hybrid/node_modules/bs-platform/linux/bsb_helper.exe
warnings = 
bsc_flags = -bs-super-errors
ppx_flags = 
g_dpkg_incls = 
g_ns = -bs-ns YourProjectName
bsb_dir_group = 0
g_lib_incls = -I . -I src/components -I /home/pv/code/REASON_EXPERIMENTS/hybrid/node_modules/reason-react/lib/ocaml
rule build_ast_from_re
  command = $bsc  $warnings -color always -bs-jsx 3 $bsc_flags -o $out -bs-syntax-only -bs-binary-ast $in
  description = [34mBuilding[39m [2m${out}[22m
build  src/components/Layout.reast : build_ast_from_re $src_root_dir/src/components/Layout.re
rule build_deps
  command = $bsdep -hash ea2c74068628be257c4034ccb2c54d39 $g_ns -g $bsb_dir_group $in
  restat = 1
  description = [34mBuilding[39m [2m${out}[22m
build  src/components/Layout.d : build_deps src/components/Layout.reast
rule ml_cmj_cmi
  command = $bsc $g_pkg_flg -color always -bs-suffix $g_lib_incls $warnings $bsc_flags -o $out $in
  dyndep = $in_e.d
  restat = 1
  description = [34mBuilding[39m [2m${out}[22m
build  src/components/Layout-YourProjectName.cmj |  src/components/Layout-YourProjectName.cmi $src_root_dir/src/components/Layout.bs.js : ml_cmj_cmi src/components/Layout.reast ||  src/components/Layout.d
  g_pkg_flg = $g_pkg_flg  -bs-package-output es6:src/components
build  src/components/Navbar.reast : build_ast_from_re $src_root_dir/src/components/Navbar.re
build  src/components/Navbar.d : build_deps src/components/Navbar.reast
build  src/components/Navbar-YourProjectName.cmj |  src/components/Navbar-YourProjectName.cmi $src_root_dir/src/components/Navbar.bs.js : ml_cmj_cmi src/components/Navbar.reast ||  src/components/Navbar.d
  g_pkg_flg = $g_pkg_flg  -bs-package-output es6:src/components
build  src/components/Greeting.reast : build_ast_from_re $src_root_dir/src/components/Greeting.re
build  src/components/Greeting.d : build_deps src/components/Greeting.reast
build  src/components/Greeting-YourProjectName.cmj |  src/components/Greeting-YourProjectName.cmi $src_root_dir/src/components/Greeting.bs.js : ml_cmj_cmi src/components/Greeting.reast ||  src/components/Greeting.d
  g_pkg_flg = $g_pkg_flg  -bs-package-output es6:src/components
rule build_package
  command = $bsc -w -49 -color always -no-alias-deps  $in
  restat = 1
  description = [34mBuilding[39m [2m${out}[22m
build  YourProjectName.cmi : build_package YourProjectName.mlmap
